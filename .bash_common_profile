#!/bin/bash

# Custom prompt
export PS1="<\u>:\W $ "

# Custom ls colors (BSD and Linux)
export LSCOLORS="gxfxcxdxbxegedabagacad"
export LS_COLORS="di=36:ln=35:so=32:pi=33:ex=31:bd=34;46:cd=34;43:su=0;41:sg=0;46:tw=0;42:ow=0;43:"

# Load common aliases
if [ -f ~/.bash_common_aliases ]; then
	. ~/.bash_common_aliases
fi

